{
  "manifest_version": 3,
  "name": "وَتَر - Watar",
  "version": "1.8",
  "description": "تحكم في سرعة وإيقاع الصوت والفيديو في كافة المواقع بهوية عربية وأداء ذكي.",
  "permissions": [
    "activeTab",
    "storage",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "512": "icon.png"
  },
  "commands": {
    "increase_speed": {
      "suggested_key": {
        "default": "Alt+Up"
      },
      "description": "زيادة السرعة"
    },
    "decrease_speed": {
      "suggested_key": {
        "default": "Alt+Down"
      },
      "description": "تقليل السرعة"
    }
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "ametwali74@mail.com",
      "strict_min_version": "109.0",
      "data_collection_permissions": {
        "required": ["none"]
      }
    }
  }
}